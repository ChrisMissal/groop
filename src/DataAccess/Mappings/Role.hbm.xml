<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CRIneta.Web.Core" namespace="CRIneta.Web.Core.Domain">
    <class name="Role" table="Facilities">
        <id name="Id" column="RoleId" type="Int32" unsaved-value="0">
            <generator class="native" /> <!-- Database will generate the key when it is needed-->
        </id>
        <property name="Name" column="Name" length="255" not-null="true" />
    </class>
</hibernate-mapping>
